{
  "ConnectionStrings": {
    "Database": "Host=localhost;Port=5434;Database=DormitoryDb;Username=postgres;Password=postgres;Include Error Detail=true"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "Dormitory ASP.NET Core Application",
      "Environment": "Development"
    }
  },

  "Jwt": {
    "Key": "SecretKeyDontShareShareShareShare",
    "Issuer": "http://localhost:5195",
    "Audience": "http://localhost:5137"
  },
  "AllowedHosts": "*"
}
